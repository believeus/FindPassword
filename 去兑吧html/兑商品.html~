      <script>
    	$(function(){
			$("#menu-two div").hide();
			$(".top-level li a").click(function(){
	            $("#menu-two div").hide();
				$(".top-level li").css("background","#FDFDFD");
			    $(".top-level li a").css("background","").css("color","#000000");
				var id=($(this).attr("value"));
				$( "#"+id).show();
				// 将当前的div背景色变为红色，字体颜色变为白色
				$(this).css("background","#E40001").css("color","#FFFFFF");
				$("#menu-two div span").css("background","").css("color","#000");
				$("#"+id+" span:first").css("background","#E40001").css("color","#FFFFFF");
			});
			
			$("#menu-two div span").click(function(){
				// 将所有的span标签设置字体为黑色
				$("#menu-two div span").css("background","").css("color","#000");
		     	$(this).css("background","").css("color","#FFF");
				$(this).css("background","#E40001");
		    });
		});
    </script>
