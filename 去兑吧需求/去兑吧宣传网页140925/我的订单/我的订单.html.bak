	<script type="text/javascript">
		 $().ready(function(){
			 
			$("#qdb-sell-add").bind('click',function(){
				
				if(parseInt($("#qdb-sell-txt").text())>=199){
					$("#qdb-sell-txt").val(199);
				}else{
					var res=parseInt($("#qdb-sell-txt").text())+1;
					$("#qdb-sell-txt").text(res);
				}
			});
			 $("#qdb-sell-reduce").bind('click',function(){
				
				if(parseInt($("#qdb-sell-txt").text())<=1){
					$("#qdb-sell-txt").text(1);
				}else{
					var res=parseInt($("#qdb-sell-txt").text())-1;
					$("#qdb-sell-txt").text(res);
				}
			});

			$("#tips").mouseover(function(){
				$("#tips_parent").css("height","70px");
				$(this).css("height","50px");
			});
			
			$("#tips").mouseout(function(){
				//alert($("#tips").val());
				if($("#tips").val() ==""){
					$("#tips_parent").css("height","40px");
					$(this).css("height","20px");
				}
				
			});
			 $('#phone-qdb').hover(function(){
				  $('.phone-qdbinfo').stop().slideDown(0);
			  },function(){
				  $('.phone-qdbinfo').stop().slideUp(200);
			  });
			   $(".menulink").hover(function(){
					$(".menuhide").css("display","block");
				},function(){

				});
				 $(".menuhide").hover(function(){

				},function(){
					$(this).css("display","none");
				});
				$("input[type='text']").focus(function(){
					$(this).css("border","1px solid #33cccc");
				});
				
		 });
	</script>
